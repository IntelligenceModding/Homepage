    "@vue/test-utils": "^2.4.6",
    "@vue/tsconfig": "^0.5.1",
    "cypress": "^13.16.0",
    "eslint": "^9.15.0",
    "eslint-plugin-cypress": "^3.6.0",
    "eslint-plugin-vue": "^9.31.0",
    "jsdom": "^24.1.3",
    "npm-run-all2": "^6.2.6",
    "sass-embedded": "^1.81.0",
    "start-server-and-test": "^2.0.8",
    "typescript": "^5.7.2",
    "unplugin-vue-components": "^0.27.4",
    "vite": "^5.4.11",
