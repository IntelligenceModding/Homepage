    "@primevue/core": "^4.2.3",
    "@primevue/themes": "^4.2.3",
    "@tailwindcss/forms": "^0.5.9",
    "autoprefixer": "^10.4.20",
    "heroicons": "^2.2.0",
    "luxon": "^3.5.0",
    "pinia": "^2.2.6",
    "postcss": "^8.4.49",
    "primeicons": "^7.0.0",
    "primevue": "^4.2.3",
    "tailwindcss": "^3.4.15",
    "tailwindcss-primeui": "^0.3.4",
    "vue": "^3.5.13",
    "vue-router": "^4.4.5"
  },
  "devDependencies": {
    "@primevue/auto-import-resolver": "^4.2.3",
